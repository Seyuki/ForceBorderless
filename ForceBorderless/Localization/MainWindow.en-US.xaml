<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="ResourceDictionaryName">Loc-en-US</system:String>

    <!--Titlebar-->
    <system:String x:Key="WindowTitle">Force borderless windows</system:String>
    <system:String x:Key="About">about</system:String>

    <!--Notify Icon-->
    <system:String x:Key="Show">Show</system:String>
    <system:String x:Key="Quit">Quit</system:String>
    
    <!--UI-->
    <system:String x:Key="Whitelist">Processes whitelist</system:String>
    <system:String x:Key="Process">Process</system:String>
    <system:String x:Key="ProcessName">Process name</system:String>
    <system:String x:Key="Add">Add</system:String>
    <system:String x:Key="Force169">Force 16:9 ratio</system:String>
    <system:String x:Key="Autostart">Start with Windows</system:String>
    
    <!--Snackbar-->
    <system:String x:Key="ProcessAdded">'{name}' added!</system:String>
    <system:String x:Key="ProcessDuplicate">'{name}' already in list</system:String>
</ResourceDictionary>