<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="ResourceDictionaryName">Loc-fr-FR</system:String>

    <!--Titlebar-->
    <system:String x:Key="WindowTitle">Forcer le fenêtré sans-bords</system:String>
    <system:String x:Key="About">à propos</system:String>

    <!--Notify Icon-->
    <system:String x:Key="Show">Afficher</system:String>
    <system:String x:Key="Quit">Quitter</system:String>

    <!--UI-->
    <system:String x:Key="Whitelist">Liste blanche des processus</system:String>
    <system:String x:Key="Process">Processus</system:String>
    <system:String x:Key="ProcessName">Nom du processus</system:String>
    <system:String x:Key="Add">Ajouter</system:String>
    <system:String x:Key="Force169">Forcer le ratio 16:9</system:String>
    <system:String x:Key="Autostart">Démarrer avec Windows</system:String>

    <!--Snackbar-->
    <system:String x:Key="ProcessAdded">'{name}' ajouté !</system:String>
    <system:String x:Key="ProcessDuplicate">'{name}' déjà dans la liste</system:String>
</ResourceDictionary>